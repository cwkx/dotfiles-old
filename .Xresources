URxvt.font:     xft:DejaVu Sans Mono for Powerline:size=10
URxvt.boldFont: xft:DejaVu Sans Mono for Powerline:style=Bold:size=10
!URxvt.font:     xft:Hack:size=10
!URxvt.boldFont: xft:Hack:style=Bold:size=10
URxvt.letterSpace: -1
URxvt.geometry: 125x40
URxvt.cursorBlink: 1
URxvt.saveLines: 8000
URxvt.scrollBar: 0

!xterm*faceName:DejaVu Sans Mono:pixelsize=12
!xterm*faceName: xft:Hack:size=10
xterm*VT100.geometry: 125x40
xterm*saveLines: 8000
xterm*scrollBar: 0

Xft.dpi: 96 
Xft.rgba: rgb
Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: 1
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintsfull
